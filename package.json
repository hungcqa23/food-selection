{
  "name": "vite-react-boilerplate",
  "author": "An Hung <hungcqa23@gmail.com>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky",
    "setup": "git init && npx husky init && shx rm .husky/pre-commit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "type-check": "tsc --noEmit",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
    "storybook:build": "storybook build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-router": "^1.120.15",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.514.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.51",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.2.9",
    "@faker-js/faker": "^9.8.0",
    "@hookform/devtools": "^4.4.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.0.5",
    "@storybook/addon-themes": "^9.0.5",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^9.0.5",
    "@storybook/react-vite": "^9.0.5",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@tanstack/react-table-devtools": "^8.21.3",
    "@tanstack/router-devtools": "^1.120.15",
    "@tanstack/router-plugin": "^1.120.15",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react-swc": "^3.10.1",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.5",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "only-allow": "^1.2.1",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "prop-types": "^15.8.1",
    "shx": "^0.4.0",
    "storybook": "^9.0.5",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "6.3.5",
    "vite-plugin-static-copy": "^3.0.0"
  },
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
